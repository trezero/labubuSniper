{
    "manifest_version": 3,
    "name": "Labubu Drop Sniper",
    "version": "1.0",
    "description": "Opens a product page at a specific time to help purchase limited drops.",
    "permissions": [
      "storage",
      "alarms",
      "tabs",
      "notifications"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }